<ServerManagerConfiguration>

  <!-- ================================================================ -->
  <!-- Filters                                                          -->
  <!-- ================================================================ -->
  <ProxyGroup name="filters">

    <!-- ================================================================ -->
    <!-- Base class for Partitioning datasets between parallel processes  -->
    <!-- ================================================================ -->
    <SourceProxy
      name="ZoltanPartitionFilter"
      class="vtkZoltanV1PartitionFilter">

      <InputProperty
        name="Input"
        command="SetInputConnection">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkPointSet"/>
        </DataTypeDomain>
        <InputArrayDomain name="input_array" optional="1"/>
      </InputProperty>

      <StringVectorProperty
        name="IdChannelArray"
        command="SetIdChannelArray"
        number_of_elements="1"
        animateable="0"
        default_values="Ids"
        label="Global points IDs">
        <ArrayListDomain
          name="array_list"
          attribute_type="Scalars"
          none_string="Not available"
          input_domain_name="input_array">
\        </ArrayListDomain>
        <Documentation>
          This property indicates the name of the array to be used for Ids
        </Documentation>
      </StringVectorProperty>

      <DoubleVectorProperty
        name="MaxAspectRatio"
        command="SetMaxAspectRatio"
        label="Max aspect ratio between bounding box axes"
        number_of_elements="1"
        animateable="1"
        default_values="5.0" >
        <DoubleRangeDomain name="range" min="1" max="20.0"/>
        <Documentation>
          Max aspect ratio allowed between any bounding box axis
        </Documentation>
      </DoubleVectorProperty>

    </SourceProxy>

  </ProxyGroup>

</ServerManagerConfiguration>
